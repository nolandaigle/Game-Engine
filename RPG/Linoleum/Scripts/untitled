bang = function(e)
	e:AddComponent("GraphicComponent")
	e:AddComponent("TransformComponent")
	e:AddComponent("CollisionComponent")
	collider = e:GetCC()
	collider:SetType("Tail1")
	graphic = e:GetGC()
	graphic:SetImage("P1.png")
end

update = function(e)
end

display = function(e)
	if graphic and showing then
		graphic:Display(e:GetTransform().x, e:GetTransform().y)
	end
end

onKeyPress = function(e,k)
end

recieveMessage = function(e, message)
	if message == "Eat" then
		e:PrintMessage("EATENN")
		showing = false
	end
end